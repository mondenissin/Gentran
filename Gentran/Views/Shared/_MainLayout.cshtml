<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <title>@ViewBag.Title - Gentran</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <link rel="shortcut icon" href="~/Images/VINCE.jpg" />
    <link rel="stylesheet" href="~/Content/bootstrap/bootstrap.css" />
    <link href="~/Content/font-awesome/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/ionicons/ionicons.min.css" rel="stylesheet" />
    <link href="~/Content/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/Content/skins/_all-skins.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/Site.css" /> 
    <link href="~/Content/jquery/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/notification/notification.css" rel="stylesheet" />
    <link href="~/Content/notification/waves-effect.css" rel="stylesheet" />
    @RenderSection("styles", required: false)
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="hold-transition skin-blue-light layout-top-nav">
    <div class="wrapper">
        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <a href="#" class="navbar-brand"><b>Gen</b>tran</a>
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                            <i class="fa fa-bars"></i>
                        </button>
                    </div>
                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-left" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li class="visitHome"><a href="@Url.Content("~/Home")">Home <span class="sr-only">(current)</span></a></li>
                            <li><a href="@Url.Content("~/Admin")">Admin</a></li>
                            <li class="visitRetrieve"><a href="@Url.Content("~/Retrieve")">PO Retrieve</a></li>
                            <li class="visitOrder"><a href="@Url.Content("~/Order")">Order</a></li>
                            <li class="visitMonitoring"><a href="@Url.Content("~/Monitoring")">Monitoring</a></li>
                        </ul>
                    </div>
                    <!-- /.navbar-collapse -->
                    <!-- Navbar Right Menu -->
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">

                            <!-- User Account Menu -->
                            <li class="user user-menu">
                                <!-- Menu Toggle Button -->
                                <a href="#">
                                    <!-- The user image in the navbar-->
                                    <img src="~/Images/shermaine.jpg" class="user-image" alt="User Image">
                                    <!-- hidden-xs hides the username on small devices so only the image appears. -->
                                    <span class="hidden-xs">Shermaine Gae Tan</span>
                                </a>
                            </li>
                            <li class="dropdown user user-menu">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-gears"></i></a>
                                <ul class="dropdown-menu">
                                    <!-- The user image in the menu -->
                                    <li class="user-header">
                                        <img src="~/Images/shermaine.jpg" class="img-circle" alt="User Image">
                                        <p>
                                            Shermaine Gae Tan  - Web Developer
                                            <small>Member since Nov. 2012</small>
                                        </p>
                                    </li>
                                    <!-- Menu Footer-->
                                    <li class="user-footer">
                                        <div class="pull-left">
                                            <a href="#" class="btn btn-default btn-flat">Profile</a>
                                        </div>
                                        <div class="pull-right">
                                            <a href="#" class="btn btn-default btn-flat">Sign out</a>
                                        </div>
                                    </li>
                                </ul>
                                </li>
                        </ul>
                    </div>
                    <!-- /.navbar-custom-menu -->
                </div>
                <!-- /.container-fluid -->
            </nav>
        </header> 
        <div class="content-wrapper">
                <section class="content">
                    @RenderBody()
                    <hr />
                    <footer>
                        <p>&copy; 2016 - Gentran</p>
                    </footer> 
                </section>
        </div>
    </div> 

        <script src="~/Scripts/jquery/jquery-2.2.3.min.js"></script>
        @*<script src="~/Scripts/plugins/raphael-min.js"></script>*@
        <script src="~/Scripts/bootstrap-js/bootstrap.js"></script>
        <script src="~/Scripts/js/app.min.js"></script>
        <script type="text/javascript" src="~/Scripts/js/master/master_ajax.js"></script>
        <script type="text/javascript" src="~/Scripts/angular/angular.js"></script>
        <script src="~/Scripts/angular/src/ui-bootstrap-tpls-0.3.0.min.js"></script>
        <script type="text/javascript" src="~/Scripts/angular/angular-route.js"></script>
        <script type="text/javascript" src="~/Scripts/js/master/master_main.js"></script>
        

        <script src="~/Scripts/angular/src/jquery.dataTables.js"></script>
        <script src="~/Scripts/angular/src/dataTables.bootstrap.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.directive.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.factory.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.options.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.renderer.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.util.js"></script>
        <script src="~/Scripts/angular/src/angular-datatables.instances.js"></script>
        <!--NOTIFICTIONS-->
        <script src="~/Scripts/notification/notifications.js"></script>
        <script src="~/Scripts/notification/waves.js"></script>
        <script src="~/Scripts/notification/notify.min.js"></script>
        <script src="~/Scripts/notification/notify-metro.js"></script>
        <script src="~/Scripts/js/master/master_balloon.js"></script>
        <script src="~/Scripts/js/scalyr/scalyr.js"></script>
        <script src="~/Scripts/js/scalyr/core.js"></script>
        <script src="~/Scripts/js/scalyr/slyEvaluate.js"></script>
        <script src="~/Scripts/js/scalyr/slyRepeat.js"></script>

        <script type="text/javascript">
            window.MyApp = {};
        </script>
        <script src="~/App/App.js"></script>
        <script type="text/javascript">
            MyApp.rootPath = '@Url.Content("~")';
        </script>
        @RenderSection("scripts", required: false)
</body>
</html>
