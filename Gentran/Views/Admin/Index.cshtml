@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Administrator";
}

@section styles{
<link href="~/Content/AdminLTE.min.css" rel="stylesheet" />
<link href="~/Content/skins/_all-skins.min.css" rel="stylesheet" />  
<link href="~/Content/ngDialog.css" rel="stylesheet" /> 

<link href="~/Content/bootstrap/bootstrap-select.css" rel="stylesheet" />    
<link href="~/Content/css/admin/reports/reports.css" rel="stylesheet" /> 
 
<link href="~/Content/daterangepicker.css" rel="stylesheet" />@*MNS20161109*@      
}

@section scripts{
    <script src="~/App/Admin/App.js"></script>
    <script src="~/App/Admin/RootViewModel.js"></script>
    <script src="~/App/Admin/ViewModels/Dashboard/DashboardModel.js"></script>
    <script src="~/App/Admin/ViewModels/Reports/MasterReportModel.js"></script>
    <script src="~/App/Admin/ViewModels/Users/UserListModel.js"></script>
    <script src="~/App/Admin/ViewModels/Prod/ProdListModel.js"></script>
    <script src="~/App/Admin/ViewModels/Cust/CustListModel.js"></script>
    <script src="~/App/Admin/ViewModels/TransLog/TransLogListModel.js"></script>

    <script src="~/Scripts/bootstrap-js/bootstrap-select.min.js"></script>
    <script src="~/Scripts/js/admin/users.js"></script> @*MNS20161011*@
    <script src="~/Scripts/angular/src/ng-fileDrop.min.js"></script >

    <script src="~/Scripts/plugins/moment.min.js"></script>@*MNS20161109*@
    <script src="~/Scripts/plugins/daterangepicker.js"></script>@*MNS20161109*@
}

@section header_menu {
<!-- Left side column. contains the logo and sidebar -->
<aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
        <!-- Sidebar user panel -->
        <div class="user-panel">
            <div class="pull-left image">
                <img src="~/Images/master/default.png" class="img-circle img_userProfPic" alt="User Image">
            </div>
            <div class="pull-left info">
                <p><span class="lbl_userfullname">Loading user...</span></p>
                <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
            </div>
        </div>
        <!-- sidebar menu: : style can be found in sidebar.less -->
            <ul class="sidebar-menu">
                <li class="header">MAIN NAVIGATION</li>
                <li>
                    <a href="#">
                        <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu ">
                        <li><button class="btn btn-link" ng-click="dashBoardv1()"><i class="fa fa-blind"></i>Pre-build v1.0</button></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-server"></i> <span>Data Maintenance</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu data-management">
                        <li><button class="btn btn-link btnuser" ng-click="userList()"><i class="fa fa-user"></i> User</button></li>
                        <li><button class="btn btn-link btnprod" ng-click="prodList()"><i class="fa fa-shopping-cart"></i> Product</button></li>
                        <li><button class="btn btn-link btncust" ng-click="custList()"><i class="fa fa-users"></i> Customer</button></li>
                        <li><button class="btn btn-link btnlogs" ng-click="logList()"><i class="fa fa-database"></i> Logs</button></li>
                        @*<li><button class="btn btn-link btbconn" ng-click="connections()"><span class="glyphicon glyphicon-sort"></span> Connections</button></li>*@
                    </ul>
                </li>
                <li>
                    <a href="#">
                        <i class="fa fa-bar-chart"></i> <span>Reports</span>
                        <span class="pull-right-container">
                            <i class="fa fa-angle-left pull-right"></i>
                        </span>
                    </a>
                    <ul class="treeview-menu ">
                        <li><button class="btn btn-link" ng-click="reportTranslog()"><i class="fa fa-file-text"></i>Reports Master</button></li>
                    </ul>
                </li>
            </ul>
    </section>
    <!-- /.sidebar -->
</aside>
}

<!-- Body -->
<section class="content-body container-fluid">
    <div class="row">
        <div class="page-table col-md-12" style="overflow-x: auto;">
            <div data-ng-view>
            </div>
        </div>
    </div>
</section>