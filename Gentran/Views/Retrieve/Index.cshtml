@{
    ViewBag.Title = "PO Retrieve";
}
<h2>@ViewData["Title"].</h2>
<h3>@ViewData["Message"]</h3>

@section styles{
    <link rel="stylesheet" href="~/Content/css/retrieve/retrieve.css" />
}

@section scripts{
    <script src="~/App/Retrieve/App.js"></script>
    <script src="~/App/Retrieve/RootViewModel.js"></script>
    <script src="~/App/Retrieve/ViewModels/SM/smModel.js"></script>
    <script src="~/App/Retrieve/ViewModels/Super8/superModel.js"></script>
}

<div data-ng-app="retrieve" data-ng-controller="rootViewModel">

    <ul class="nav nav-tabs skin-blue">
        <li><a data-toggle="tab" href="/#FTP"><img src="~/Images/FTP.png" style="width:150px;height:125px;" /><label class="tab-label">FTP</label></a></li>
        <li><a data-toggle="tab" href="/#E-Comm"><img src="~/Images/ECOM.png" style="width:150px;height:125px" /><label class="tab-label">E-Comm</label></a></li>
        <li><a data-toggle="tab" href="/#API"><img src="~/Images/API.png" style="width:150px;height:125px" /><label class="tab-label">Web Service</label></a></li>
    </ul>

    <div class="tab-content">
        <div id="FTP" class="tab-pane fade ">
            <h3>Outlets</h3>
            <button ng-click="smFile()"><img src="~/Images/outlets/sm.png" style="width:100px;height:75px;" /></button>
            <button ng-click="superFile()"><img src="~/Images/outlets/super8.jpg" style="width:100px;height:75px" /></button>
        </div>
        <div id="E-Comm" class="tab-pane fade">
            <h3>Outlets</h3>
            <button ng-click="smFile()"><img src="~/Images/outlets/sm.png" style="width:100px;height:75px;" /></button>
        </div>
        <div id="API" class="tab-pane fade">
            <h3>Outlets</h3>
            <button ng-click="smFile()"><img src="~/Images/outlets/sm.png" style="width:100px;height:75px;" /></button>
        </div>
    </div>

    <div ng-view></div>
</div>